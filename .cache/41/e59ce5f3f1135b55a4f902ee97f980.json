ht: normal;\n        line-height: 20px;\n        padding-top: 8px;\n        padding-bottom: 8px;\n        border-bottom: 1px solid rgba(0, 0, 0, 0.12);\n        text-decoration: none;\n        color: rgba(0, 0, 0, 0.87);\n      }\n\n      &:first-child {\n        border-top: 1px solid rgba(0, 0, 0, 0.12);\n      }\n\n      .topic-link {\n        position: absolute;\n        top: 0;\n        left: 0;\n        width: 100%;\n        height: 100%;\n\n        &:hover {\n          background-color: rgba($sub-color, 0.3);\n        }\n      }\n    }\n  }\n}\n","_archives.scss":"// archives\n.archives {\n    margin: 0;\n  \n    .archives-title {\n      text-align: center;\n      font-size: 32px;\n      font-weight: 600;\n      margin-bottom: 50px;\n    }\n  \n    .archives-contents {\n      max-width: $max-width;\n      padding: $padding;\n      margin: $margin;\n      display: flex;\n      flex-wrap: wrap;\n      justify-content: space-between;\n  \n      .contents-container {\n        position: relative;\n        width: calc(238 / 1040 * 100%);\n  \n        .image-wrap {\n          margin-bottom: 8px;\n          background-color: #000000;\n          border-radius: 3px;\n  \n          .contents-image {\n            position: relative;\n            border-radius: 3px;\n            opacity: 0.74;\n          }\n        }\n  \n        .description {\n          font-size: 14px;\n          font-weight: 600;\n          margin: 0 0 8px;\n        }\n  \n        .description-link {\n          text-decoration: none;\n          color: rgba(0, 0, 0, 0.87);\n  \n          &:hover {\n            color: rgba(0, 0, 0, 0.54);\n          }\n        }\n  \n        .date {\n          text-align: right;\n          font-size: 10px;\n          font-weight: normal;\n          color: rgba(0, 0, 0, 0.54);\n          margin: 0 0 24px;\n        }\n  \n        .archives-link {\n          position: absolute;\n          top: 0;\n          left: 0;\n          width: 100%;\n          height: 100%;\n  \n          &:hover {\n            background: rgba($sub-color, 0.3);\n          }\n        }\n  \n      }\n    }\n  }","_more.scss":"// more\n.button {\n    text-align: center;\n    display: flex;\n    justify-content: center;\n  \n    .more-button {\n      position: relative;\n      margin-top: 24px;\n      margin-bottom: 100px;\n      width: 300px;\n      height: 43px;\n      line-height: 43px;\n      border: 1px solid $main-color;\n      border-radius: 50px;\n  \n      &:hover {\n        background-color:  $main-color;\n      }\n  \n      .more-link {\n        position: absolute;\n        display: inline-block;\n        vertical-align: middle;\n        top: 0;\n        left: 0;\n        width: 100%;\n        height: 100%;\n        text-decoration: none;\n        color: $main-color;\n  \n        &:hover {\n          color: $sub-color;\n        }\n  \n      }\n    }\n  }","_media_query.scss":"@media (max-width: 480px) {\n    // menu-bar\n    .menu-bar {\n      ul {\n        padding: 0 7%;\n      }\n    }\n  \n    .title {\n      span {\n        top: 170px;\n      }\n    }\n  \n    // news-image\n    .news {\n      display: flex;\n      flex-direction: column;\n  \n      .news-image {\n        width: 100%;\n  \n        .image-container {\n          width: 100%;\n          height: auto;\n          margin: 0;\n  \n          &::before {\n            @include background-color;\n            bottom: 3px;\n          }\n  \n          span {\n            top: 20px;\n            left: 113px;\n          }\n        }\n      }\n      .news-topics {\n        width: 100%;\n        margin: 14px 0 70px 0;\n  \n        .topics-container {\n          text-align: center;\n          margin: 5px 0 0 0;\n          width: 100%;\n        }\n      }\n    }\n  }\n  "},"lineCount":null}},"error":null,"hash":"9ce28d2f144cae7d16c2bd2a4df5045d","cacheData":{"env":{}}}