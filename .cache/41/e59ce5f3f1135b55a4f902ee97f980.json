x 10px #000000;\n\n@mixin background-color {\n  background-color: rgba(0, 0, 0, 0.26);\n  position: absolute;\n  top: 0;\n  right: 0;\n  bottom: 0;\n  left: 0;\n  content: \" \";\n}\n\nbody {\n  margin: 0;\n  font-family: \"Lato\", sans-serif;\n}\n\n// header\nheader {\n  position: relative;\n  text-align: center;\n  width: 100%;\n}\n\n.title {\n  background: url(\"background-image.png\") no-repeat;\n  height: 480px;\n  background-position: center;\n  margin-bottom: 80px;\n  position: relative;\n\n  span {\n    display: inline-block;\n    color: $sub-color;\n    font-family: Lato;\n    font-size: 64px;\n    font-style: normal;\n    font-weight: 200;\n    line-height: 77px;\n    letter-spacing: 0;\n    text-shadow: $shadow;\n    top: 200px;\n    left: 0;\n    right: 0;\n    margin: auto;\n    position: absolute;\n    z-index: 1;\n  }\n\n  &::before {\n    @include background-color;\n  }\n}\n\n.menu-bar {\n  z-index: 1;\n  position: absolute;\n  top: 0;\n  height: 60px;\n  width: 100%;\n\n  ul {\n    font-size: 14px;\n    font-style: light;\n    font-weight: 300;\n    padding: 0 14%;\n    display: flex;\n    list-style: none;\n    align-items: center;\n    margin: 19px 0 19px 0;\n\n    li {\n      align-self: center;\n    }\n\n    li:first-child {\n      font-size: 18px;\n      font-style: regular;\n      font-weight: normal;\n      margin-right: auto;\n    }\n\n    a {\n      color: $main-color;\n      text-decoration: none;\n      padding: 0 8px 0 8px;\n    }\n  }\n  &::before {\n    background: $sub-color;\n    opacity: 0.2;\n    position: absolute;\n    top: 0;\n    right: 0;\n    bottom: 0;\n    left: 0;\n    content: \" \";\n  }\n}\n\n.slide-button {\n  position: absolute;\n  bottom: 24px;\n  display: flex;\n  width: 100%;\n  justify-content: center;\n\n  &__circle {\n    display: inline-block;\n    width: 8px;\n    height: 8px;\n    border-radius: 50%;\n    background-color: $sub-color;\n    opacity: 0.8;\n    margin: 0 4px;\n  }\n}\n\n.slide-button__circle—-selected {\n  border: 1px solid $main-color;\n}\n\n// 本当は以下のようにやりたかった\n// .slide-button {\n//   position: absolute;\n//   bottom: 24px;\n//   display: flex;\n//   width: 100%;\n//   justify-content: center;\n\n//   &__circle {\n//     display: inline-block;\n//     width: 8px;\n//     height: 8px;\n//     border-radius: 50%;\n//     background-color: $sub-color;\n//     opacity: 0.8;\n//     margin: 0 4px;\n\n//     // ここ何故か効いてない\n//     &--selected {\n//       border: 1px solid $main-color;\n//     }\n//   }\n// }\n\n// news-image\n.news {\n  overflow: hidden;\n\n  .news-image {\n    width: 50vw;\n    float: left;\n\n    .image-container {\n      float: right;\n      position: relative;\n      width: 500px;\n      height: 120px;\n      margin-right: 17px;\n\n      &::before {\n        @include background-color;\n      }\n    }\n\n    img {\n      width: 100%;\n    }\n\n    span {\n      position: absolute;\n      top: 35%;\n      left: 41%;\n      color: $sub-color;\n      font-size: 32px;\n      font-style: light;\n      font-weight: 300;\n      text-shadow: $shadow;\n    }\n  }\n}\n\n// news-topics\n.news-topics {\n  width: 50vw;\n  float: right;\n  margin-bottom: 70px;\n\n  .topics-container {\n    margin: 5px 20px 0 20px;\n    padding: 0;\n    list-style: none;\n    font-size: 16px;\n    width: 500px;\n\n    li {\n      font-family: Noto Sans JP;\n      font-size: 14px;\n      font-style: regular;\n      font-weight: normal;\n      color: rgba(0, 0, 0, 0.87);\n      line-height: 20px;\n      padding-top: 8px;\n      padding-bottom: 8px;\n      border-bottom: 1px solid rgba(0, 0, 0, 0.12);\n    }\n\n    &:first-child {\n      border-top: 1px solid rgba(0, 0, 0, 0.12);\n    }\n  }\n}\n\n// archives\n.archives {\n  margin: 0;\n\n  .archives-title {\n    text-align: center;\n    font-size: 32px;\n    font-style: semibold;\n    font-weight: 600;\n    margin-bottom: 50px;\n  }\n\n  .archives-contents {\n    text-align: center;\n    padding: 0 7%;\n\n    .contents-container {\n      text-align: left;\n      position: relative;\n      width: 238px;\n      display: inline-block;\n      margin: 0 12px;\n\n      .image-wrap {\n        margin-bottom: 8px;\n        background-color: #000000;\n        border-radius: 3px;\n        height: 120px;\n\n        .contents-image {\n          position: relative;\n          border-radius: 3px;\n          opacity: 0.74;\n        }\n      }\n\n      .description {\n        font-size: 14px;\n        font-style: semibold;\n        font-weight: 600;\n        margin: 0 0 8px;\n        color: rgba(0, 0, 0, 0.87);\n      }\n\n      .date {\n        text-align: right;\n        font-size: 10px;\n        font-style: Regular;\n        font-weight: normal;\n        color: rgba(0, 0, 0, 0.54);\n        margin: 0 0 24px;\n      }\n    }\n  }\n}\n\n// more\n.button {\n  text-align: center;\n\n  .more-button {\n    margin: 24px auto 100px;\n    width: 300px;\n    border: 1px solid $main-color;\n    border-radius: 50px;\n\n    p {\n      margin: 14px auto;\n    }\n  }\n}\n\n@media (max-width: 320px) {\n  // news-image\n  .news-image {\n    width: 100vw;\n    float: none;\n  }\n\n  // news-topics\n  .news-topics {\n    width: 50vw;\n    float: none;\n  }\n}"},"lineCount":null}},"error":null,"hash":"972d52f40b30d74f2a6f89c61ab3af4d","cacheData":{"env":{}}}